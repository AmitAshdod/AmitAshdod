package org.firstinspires.ftc.teamcode;

import com.qualcomm.robotcore.eventloop.opmode.Autonomous;
import com.qualcomm.robotcore.eventloop.opmode.OpMode;
import com.qualcomm.robotcore.hardware.DcMotor;


@Autonomous

public class AutonomusMode extends OpMode {

 DcMotor motorFrontLeft;
 DcMotor motorFrontRight;
 DcMotor motorBackLeft;
 DcMotor motorBackRight;

 public class init() {

         motorFrontLeft = hardwareMap.get("motorFrontLeft");
         motorFrontRight = hardwareMap.get("motorFrontRight";
         motorBackLeft = hardwareMap.get("motorBackLeft");
         motorBackLeft = hardwareMap.get("motorBackRight");

 }
public class loop(){

     motorFrontRight.setPower(.75);
     motorFrontLeft.setPower(.75);
     motorBackRight.setPower(.75);
     motorBackLeft.setPower(.75);

     motorFrontRight.Sleep();
     

}
